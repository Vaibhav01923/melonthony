generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Album {
  id           String   @id @map("_id")
  score        Json
  album_clean  String
  artist_clean String
  genres       String[]
  year         String
  vref         String
  cover_url    String
  score_num    Int?
  score_text   String?

  @@map("albums")
}
